#include <iostream>

using namespace std;

int main()
{
    int arre[3][3]={{3,4,6},{8,8,9},{1,2,3}};
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cout<<arre[i][j];
        }
        cout<<"\n";
    }
    cout<<"\nIMPRIMIENDO DIAGONAL INVERTIDA\n";
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            if(i+j==2){
                cout<<arre[i][j];
            }
        }
        cout<<"\n";
    }
    return 0;
}
