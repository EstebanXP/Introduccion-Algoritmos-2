#include <iostream>

using namespace std;

int main()
{
    int arre[3][3]={{3,2,4},{5,6,1},{8,6,8}};
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
                cout<<arre[i][j];
        }
        cout<<"\n";
    }
    cout<<"\nIMPRIMIENDO DIAGONAL PRINCIPAL \n";
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            if(i==j){
                cout<<arre[i][j]<<"\n";
            }
        }
    }
    return 0;
}
